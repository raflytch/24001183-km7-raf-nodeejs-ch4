<h1>Users List</h1>
<table class="table table-striped">
  <thead>
    <tr>
      <th>Name</th>
      <th>Photo Profile</th>
      <th>Age</th>
      <th>Phone Number</th>
    </tr>
  </thead>
  <tbody>
    <% users.map(user => { %>
    <tr>
      <td><%= user.firstName %> <%= user.lastName%></td>
      <td>
        <img
          src="<%= user.photoProfile %>"
          class="rounded"
          alt=""
          style="width: 100px"
        />
      </td>
      <td><%= user.age %></td>
      <td><%= user.phoneNumber %></td>
    </tr>
    <% }) %>
  </tbody>
</table>
